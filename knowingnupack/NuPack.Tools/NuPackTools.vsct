<?xml version="1.0" encoding="utf-8"?>
<CommandTable xmlns="http://schemas.microsoft.com/VisualStudio/2005-10-18/CommandTable" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <Extern href="stdidcmd.h"/>
  <Extern href="vsshlids.h"/>
  <Extern href="msobtnid.h"/>

  <Commands package="guidPowerConsolePkg">

    <Menus>
      <!-- This is the toolbar in the console -->
      <Menu guid="guidPowerConsoleCmdSet" id="idToolbar" type="ToolWindowToolbar">
        <CommandFlag>DefaultDocked</CommandFlag>
        <Strings>
          <ButtonText>Package Manager Console</ButtonText>
        </Strings>
      </Menu>
    </Menus>
    
    <Groups>
      <Group guid="guidPowerConsoleCmdSet" id="idToolbarHostsGroup" priority="0x1000">
        <Parent guid="guidPowerConsoleCmdSet" id="idToolbar"/>
      </Group>
      <Group guid="guidPowerConsoleCmdSet" id="idToolbarProjectGroup" priority="0x1500">
        <Parent guid="guidPowerConsoleCmdSet" id="idToolbar"/>
      </Group>
      <Group guid="guidPowerConsoleCmdSet" id="idToolbarCommandsGroup" priority="0x2000">
        <Parent guid="guidPowerConsoleCmdSet" id="idToolbar"/>
      </Group>
      <Group guid="guidNuPack_Dialog_PackageCmdSet" id="MyMenuGroup" priority="0x0600">
          <Parent guid="guidSHLMainMenu" id="IDM_VS_CTXT_REFERENCEROOT"/>
      </Group>
    </Groups>
    
    <Buttons>
      <Button guid="guidPowerConsoleCmdSet" id="cmdidPowerConsole" priority="0x0100" type="Button">
        <Parent guid="guidSHLMainMenu" id="IDG_VS_WNDO_OTRWNDWS1"/>
        <Icon guid="guidToolbarImages" id="bmpPowerConsole" />
        <Strings>
          <ButtonText>Package Manager Console</ButtonText>
        </Strings>
      </Button>

      <Button guid="guidPowerConsoleCmdSet" id="cmdidSourceSettings" priority="0x1020" type="Button">
        <Parent guid="guidPowerConsoleCmdSet" id="idToolbarHostsGroup"/>
        <Icon guid="guidToolbarImages2" id="bmpSourceSettings" />
        <Strings>
          <ButtonText>&amp;Manage package sources</ButtonText>
        </Strings>
      </Button>
      
      <Button guid="guidPowerConsoleCmdSet" id="cmdidClearHost" priority="0x1010" type="Button">
        <Parent guid="guidPowerConsoleCmdSet" id="idToolbarCommandsGroup"/>
        <Icon guid="guidToolbarImages" id="bmpClearConsole" />
        <Strings>
          <ButtonText>&amp;Clear Console</ButtonText>
        </Strings>
      </Button>

      <Button guid="guidNuPack_Dialog_PackageCmdSet" id="cmdidASPNETPackages" priority="0x0100" type="Button">
          <Parent guid="guidSHLMainMenu" id="IDG_VS_CTXT_REFERENCE" />
          <CommandFlag>DefaultInvisible</CommandFlag>
          <Strings>
              <CommandName>cmdidASPNETPackages</CommandName>
              <ButtonText>Add Package Reference...</ButtonText>
          </Strings>
      </Button>
    </Buttons>

    <Combos>
      <Combo guid="guidPowerConsoleCmdSet" id="cmdidSources" idCommandList="cmdidSourcesList" type="IndexCombo" defaultWidth="270" priority="0x1005">
        <Parent guid="guidPowerConsoleCmdSet" id="idToolbarHostsGroup" />
        <CommandFlag>IconAndText</CommandFlag>
        <Strings>
            <ButtonText>Package &amp;source:</ButtonText>
        </Strings>
      </Combo>

      <Combo guid="guidPowerConsoleCmdSet" id="cmdidProjects" idCommandList="cmdidProjectsList" type="IndexCombo" defaultWidth="200" priority="0x1010">
        <Parent guid="guidPowerConsoleCmdSet" id="idToolbarProjectGroup" />
        <CommandFlag>IconAndText</CommandFlag>
        <Strings>
            <ButtonText>Default &amp;project:</ButtonText>
        </Strings>
      </Combo>
    </Combos>
   
    <Bitmaps>
      <Bitmap guid="guidToolbarImages" href="Resources\Toolbar.png"/>
      <Bitmap guid="guidToolbarImages2" href="Resources\SourceSettings.png" />
    </Bitmaps>
  </Commands>

  <VisibilityConstraints>
    <VisibilityItem guid="guidNuPack_Dialog_PackageCmdSet" id="cmdidASPNETPackages" context="UICONTEXT_SolutionExists" />
  </VisibilityConstraints>

  <CommandPlacements>
      <CommandPlacement guid="guidNuPack_Dialog_PackageCmdSet" id="cmdidASPNETPackages" priority="0xF100">
          <Parent guid="guidSHLMainMenu" id="IDG_VS_CTXT_REFROOT_ADD"/>
      </CommandPlacement>
      <CommandPlacement guid="guidNuPack_Dialog_PackageCmdSet" id="cmdidASPNETPackages" priority="0xF100">
          <Parent guid="guidSHLMainMenu" id="IDG_VS_CTXT_PROJECT_ADD"/>
      </CommandPlacement>
      <CommandPlacement guid="guidNuPack_Dialog_PackageCmdSet" id="cmdidASPNETPackages" priority="0xF100">
          <Parent guid="guidSHLMainMenu" id="IDG_VS_PROJ_OPTIONS"/>
      </CommandPlacement>
      <CommandPlacement guid="guidNuPack_Dialog_PackageCmdSet" id="cmdidASPNETPackages" priority="0xF100">
          <Parent guid="guidVenusCmdId" id="IDG_VENUS_CTX_REFERENCE"/>
      </CommandPlacement>
      <CommandPlacement guid="guidNuPack_Dialog_PackageCmdSet" id="cmdidASPNETPackages" priority="0xF100">
          <Parent guid="guidVenusCmdId" id="IDG_CTX_REFERENCE"/>
      </CommandPlacement>
  </CommandPlacements>

  <KeyBindings>
    <KeyBinding guid="guidPowerConsoleCmdSet" id="cmdidPowerConsole" editor="guidVSStd97" mod1="Control" key1="W" key2="Z" />
    <KeyBinding guid="guidPowerConsoleCmdSet" id="cmdidPowerConsole" editor="guidVSStd97" mod1="Control" key1="W" mod2="Control" key2="Z" />
  </KeyBindings>
  
